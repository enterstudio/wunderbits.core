{
  "options": {
    "compress": true,
    "twoFiles": true,
    "useSourceUrl": false,
    "logLevel": 3,
    "baseUrl": "public",
    "buildDir": "dist"
  },

  "core": {
    "options": {
      "packages": [{
        "name": "wunderbits/core",
        "location": "."
      }],
      "include": [
        "wunderbits/core/WBClass",
        "wunderbits/core/WBEventEmitter",
        "wunderbits/core/WBDeferred"
      ]
    }
  }
}